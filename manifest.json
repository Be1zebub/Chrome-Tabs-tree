{
    "manifest_version": 3,
    "name": "Tabs Tree",
    "version": "1.0",
	"author": "https://t.me/be1zebub",
    "description": "A tree style tabs navigator with searchbar",
    "permissions": [
        "tabs",
		"tabGroups",
		"activeTab",
        "commands",
		"bookmarks"
    ],
	"host_permissions": [
		"http://*/*",
		"https://*/*"
	],
    "action": {
        "default_popup": "popup.html"
    },
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Alt+Q"
            },
            "description": "Open the popup"
        }
    },
	"icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}